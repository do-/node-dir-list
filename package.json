{
  "name": "fs-iterators",
  "version": "1.0.0",
  "description": "Iterating over file paths in a given set of directories, lambda filters",
  "main": "index.js",
  "files": [
    "/lib",
    "/badges"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageReporters": [
      "clover",
      "json",
      "lcov",
      "text",
      "json-summary"
    ],
    "verbose": false,
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/data",
      "/__tests__/lib"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/do-/node-dir-list.git"
  },
  "keywords": [
    "fs",
    "iterator"
  ],
  "devDependencies": {
    "jest": "^29.3.1"
  },
  "author": "Dmitry Ovsyanko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/do-/node-dir-list/issues"
  },
  "homepage": "https://github.com/do-/node-dir-list#readme"
}
